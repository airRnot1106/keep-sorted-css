{
  "name": "@airrnot/keep-sorted-css",
  "version": "0.0.1",
  "description": "Custom command for eslint-plugin-command to sort CSS properties",
  "keywords": [
    "css-in-js",
    "eslint-plugin-command",
    "sort"
  ],
  "homepage": "https://github.com/airRnot1106/keep-sorted-css#readme",
  "bugs": {
    "url": "https://github.com/airRnot1106/keep-sorted-css/issues"
  },
  "license": "MIT",
  "author": "airRnot <airrnot.dev@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airRnot1106/keep-sorted-css.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": "./dist/index.mjs",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  },
  "scripts": {
    "build": "tsdown",
    "lint": "oxlint --type-aware --type-check --fix && eslint --fix",
    "format": "oxfmt",
    "test": "vitest",
    "check": "npm-run-all lint format"
  },
  "dependencies": {
    "eslint-plugin-command": "^3.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.0.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "^1.36.0",
    "lefthook": "^2.0.13",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.21.0",
    "oxlint": "^1.36.0",
    "oxlint-tsgolint": "^0.10.1",
    "publint": "^0.3.16",
    "tsdown": "0.19.0-beta.2",
    "typescript": "^5.9.3",
    "unplugin-unused": "^0.5.6",
    "vitest": "^4.0.16"
  },
  "packageManager": "pnpm@10.26.1"
}
